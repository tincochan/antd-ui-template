{
  "name": "antd-ui-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lt": "bash scripts/release.sh && vue-cli-service build --mode staging",
    "start": "vue-cli-service serve --open --copy --fix",
    "build": "yarn lint && vue-cli-service build",
    "lint": "vue-cli-service lint --mode production"
  },
  "dependencies": {
    "@xiyun/ant-design-ui": "^1.2.10",
    "@xiyun/util-style": "^1.1.12",
    "@xiyun/utils": "^1.1.7",
    "ant-design-vue": "^1.2.4",
    "eslint-plugin-prettier": "^3.1.0",
    "lottie-web": "^5.5.6",
    "vue": "^2.6.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import": "^1.11.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.4",
    "vue-template-compiler": "^2.6.9"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "prettier --write",
      "vue-cli-service lint --mode production",
      "git add"
    ]
  }
}
